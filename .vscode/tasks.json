{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "compile",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-Wall",
                "-I", "${workspaceFolder}",
                "-o", "${workspaceFolder}/main",
                "dum.cc",
                "-L", "${workspaceFolder}", "-Wl,--no-as-needed",
                "-lAnalysisFramework", "-lAnalysisObjects",
                "-lAnalysisUtilities", "-lEventDump", "-lParticleMass", 
                "-lParticleLifetime",
                "`root-config --libs`"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}